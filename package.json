{
	"name": "project-cicero",
	"version": "0.1.0",
	"description": "A Wordpress theme for Project Cicero, an annual non-profit book drive designed to create and supplement classroom libraries in under-resourced New York City public schools.",
	"main": "index.js",
	"scripts": {
		"scss": "node-sass --include-path scss sass/style.scss style.css --output-style=expanded --indent-type=tab --indent-width=1",
		"autoprefixer": "postcss style.css -u autoprefixer -o style.css --no-map",
		"stylelint": "stylelint 'style.css'",
		"stylefix": "stylelint 'style.css' --fix",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.1",
		"browserslist": "^2.1.5",
		"node-sass": "^4.5.3",
		"postcss-cli": "^4.1.0",
		"stylelint": "^7.12.0",
		"stylelint-config-wordpress": "^11.0.0"
	},
	"stylelint": {
		"defaultSeverity": "warning",
		"extends": "stylelint-config-wordpress"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kalus/project-cicero.git"
	},
	"keywords": [
		"Wordpress",
		"Sass",
		"_s",
		"Non-Profit",
		"GPL-2.0"
	],
	"author": "Lukas Fauset and Carla Stockalper",
	"license": "GPL-2.0",
	"bugs": {
		"url": "https://github.com/Kalus/project-cicero/issues"
	},
	"homepage": "https://github.com/Kalus/project-cicero#readme"
}
